# c-programming-192211378
#include <stdio.h>

int main() {

    int n, i;

    printf("Enter the number upto which you want to generate even numbers: ");

    scanf("%d", &n);

    printf("The even numbers from 2 to %d are: \n", n);

    for (i = 2; i <= n; i += 2) {

        printf("%d\n", i);

    }

    return 0;

}
