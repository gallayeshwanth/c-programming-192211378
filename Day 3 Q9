# c-programming-192211378
#include <stdio.h>

#include <math.h>

int main() {

  int i, n, sum = 0;

  printf("Enter the value of n: ");

  scanf("%d", &n);

  for (i = 1; i <= n; i += 2) {

    sum += pow(i, 2);

  }

  printf("Sum of squares of odd numbers from 1 to %d: %d\n", n, sum);

  return 0;

}
