# c-programming-192211378
#include <stdio.h>

int main()

{

    int n, i, f[100];

    

    printf("Enter the number of terms: ");

    scanf("%d", &n);

    

    f[0] = 0;

    f[1] = 1;

    

    for (i = 2; i < n; i++)

    {

        f[i] = f[i-1] + f[i-2];

    }

    

    printf("The Fibonacci series is: \n");

    

    for (i = 0; i < n; i++)

    {

        printf("%d, ", f[i]);

    }

    

    return 0;

}
